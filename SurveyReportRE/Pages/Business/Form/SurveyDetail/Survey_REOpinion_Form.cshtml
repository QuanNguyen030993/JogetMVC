@page "{pageNum:int?}/{jsonConfig?}"
@model Survey_REOpinion_FormModel
@{
    Layout = null;
}
<script>

    $(function () {
          var _id = @Convert.ToInt32(ViewData["Id"]);
          var _jsonConfig = '@Html.Raw(ViewData["JsonConfig"])';
          var _surveyData = new Object();
          if (_jsonConfig) _surveyData = JSON.parse(_jsonConfig);
          var _formConfig = new MFormOption();
          _formConfig.cloneUrl = '/Business/Form/SurveyDetail/Survey_REOpinion_Form/' + _id;
          _formConfig.tabCode = 'Survey_REOpinion_Form';
          _formConfig.tabName = "";
          _formConfig.fieldsFilterByTab = "REOpinion";
          _formConfig.schemeBasedOn = "@Survey_REOpinion_FormModel.ModelName";
          _formConfig.allowFormActionButton = false;
          _formConfig.originModelName = "@Survey_REOpinion_FormModel.ModelName";
          _formConfig.formElementName = "Survey_REOpinion_Form";
          _formConfig.labelLocation = "top";
          _formConfig.colCount = 1;
        _formConfig.outlineForm = { isUse: true, surveyTypeId: _surveyData.surveyTypeId, isOutlineChecked: false };
        _formConfig.isReadOnly = _surveyData.isReadOnly;
        new REOpinionForm(_id, null, _formConfig);
    });
</script>
<div id="form_Survey_REOpinion_Form_@Convert.ToInt32(ViewData["Id"])"></div>

