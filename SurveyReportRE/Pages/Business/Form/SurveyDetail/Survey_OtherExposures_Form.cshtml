@page "{refPageNum:int?}/{pageNum:int?}/{jsonConfig?}"
@model Survey_OtherExposures_FormModel
@{
    Layout = null;
}
<script>

    $(function () {
          var _id = @Convert.ToInt32(ViewData["Id"]);
        var _fkId = @Convert.ToInt32(ViewData["FKId"]);


        var _jsonConfig = '@Html.Raw(ViewData["JsonConfig"])';
        var _surveyData = new Object();
        if (_jsonConfig) _surveyData = JSON.parse(_jsonConfig); 

        if (_id)
        $.ajax({
            url: `api/OtherExposures/GetAdditionalOutline/${_id}`,
            type: 'GET',
            async: false,
            success: function (response) {
                _surveyData.additionalOutline = response;
            },
            error: function (exception) {
            }
        });
        var _formConfig = new MFormOption(_surveyData);
          _formConfig.cloneUrl = '/Business/Form/SurveyDetail/Survey_OtherExposures_Form/' + _id;
          _formConfig.tabCode = 'Survey_OtherExposures_Form';
          _formConfig.tabName = "";
          _formConfig.fieldsFilterByTab = "OtherExposures";
          _formConfig.allowFormActionButton = false;
          _formConfig.originModelName = "@Survey_OtherExposures_FormModel.ModelName";
          _formConfig.formElementName = "Survey_OtherExposures_Form";
          _formConfig.colCount = 1;
          _formConfig.labelLocation = "top";
          _formConfig.schemeBasedOn = `@(Survey_OtherExposures_FormModel.SchemeModelName)`;
          _formConfig.refFieldId = _fkId;
        _formConfig.refFieldName = `@(Survey_OtherExposures_FormModel.FKModelName)Id`;
        _formConfig.isChildForeignKey = true;
        _formConfig.outlineForm = {isUse: true, surveyTypeId: _surveyData.surveyTypeId, isOutlineChecked: false, isOutlineDynamic: true, isAllowRemoveOutline: true };
        _formConfig.isAllowAddOutline = true;
        _formConfig.isReadOnly = _surveyData.isReadOnly;
        new OtherExposuresForm(_id, null, _formConfig, _formConfig);
    });
</script>
<div id="form_Survey_OtherExposures_Form_@(Convert.ToInt32(ViewData["FKId"]))_@(Convert.ToInt32(ViewData["Id"]))"></div>

