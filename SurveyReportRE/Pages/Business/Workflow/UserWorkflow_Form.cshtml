@page "{pageNum:int?}"
@model UserWorkflow_FormModel
@{


}
<script>
    $(function () {
        var _id = @Convert.ToInt32(ViewData["Id"]);
        var _formConfig = new MFormOption();
        _formConfig.cloneUrl = '/Business/MasterData/UserWorkflow_Form/' + _id;
        _formConfig.tabCode = `form_UserWorkflow_Form_${_id}`;
        _formConfig.tabName = '@HttpContext.Request.Query["Caption"].ToString()';
        _formConfig.originModelName = "@UserWorkflow_FormModel.ModelName";
        _formConfig.formElementName = "@UserWorkflow_FormModel.ModelName";
        _formConfig.schemeBasedOn = `@(UserWorkflow_FormModel.SchemeModelName)`;
        _formConfig.allowFormActionButton = true;
        _formConfig.colCount = 4;
        _formConfig.isCustomQuery = true;
        new UserWorkflowForm(_id, null, _formConfig);
    });
</script>
<div id="form_@(UserWorkflow_FormModel.ModelName)_@Convert.ToInt32(ViewData["Id"])" class="dExForm"></div>

