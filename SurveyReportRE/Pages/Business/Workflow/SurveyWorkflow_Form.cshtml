@page "{pageNum:int?}"
@model SurveyWorkflow_FormModel
@{


}
<script>
    $(function () {
        var _id = @Convert.ToInt32(ViewData["Id"]);

        var surveyData = new Object();
         $.ajax({
                url: `api/Survey/GetSingleInclude/${_id}`,
                type: 'GET',
                async: false,
                success: function (response) {
                    surveyData = response;
                },
                error: function (exception) {
                }
            });

        var _formConfig = new MFormOption(surveyData);
        _formConfig.cloneUrl = '/Business/MasterData/SurveyWorkflow_Form/' + _id;
        _formConfig.tabCode = `form_SurveyWorkflow_Form_${_id}`;
        _formConfig.tabName = '@HttpContext.Request.Query["Caption"].ToString()';
        _formConfig.originModelName = "@SurveyWorkflow_FormModel.ModelName";
        _formConfig.formElementName = "@SurveyWorkflow_FormModel.ModelName";
        _formConfig.schemeBasedOn = `@(SurveyWorkflow_FormModel.SchemeModelName)`;
        _formConfig.allowFormActionButton = true;
        _formConfig.colCount = 4;
        _formConfig.isCustomQuery = true;
        new SurveyWorkFlowForm(_id, null, _formConfig, _formConfig);
         
    });
</script>
<div id="form_@(SurveyWorkflow_FormModel.ModelName)_@(Convert.ToInt32(ViewData["Id"]))" class="dExForm"></div>

